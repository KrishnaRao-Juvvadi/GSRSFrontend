{
  "name": "gsrs-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build-file-select": "ng build --prod file-select && cd dist/file-select && npm pack",
    "build-jsdraw-wrapper": "ng build --prod jsdraw-wrapper && cd dist/jsdraw-wrapper && npm pack",
    "build-ketcher-wrapper": "ng build --prod ketcher-wrapper && cd dist/ketcher-wrapper && npm pack",
    "build-libraries": "npm run build-file-select && npm run build-jsdraw-wrapper && npm run build-ketcher-wrapper",
    "start:abase:dev": "npm run build-libraries && ng serve --aot=true --configuration=abase.dev",
    "start:abase:local": "npm run build-libraries && ng serve --aot=true --configuration=abase.local",
    "start:abase:pre-prod": "npm run build-libraries && ng serve --aot=true --configuration=abase.pre-prod",
    "start:abase:prod": "npm run build-libraries && ng serve --aot=true --configuration=abase.prod",
    "start:fda:dev": "npm run build-libraries && ng serve --aot=true --configuration=fda.dev",
    "start:fda:local": "npm run build-libraries && ng serve --aot=true --configuration=fda.local",
    "start:fda:pre-prod": "npm run build-libraries && ng serve --aot=true --configuration=fda.pre-prod",
    "start:fda:prod": "npm run build-libraries && ng serve --aot=true --configuration=fda.prod",
    "start:gsrs:dev": "npm run build-libraries && ng serve --aot=true --configuration=gsrs.dev",
    "start:gsrs:local": "npm run build-libraries && ng serve --aot=true --configuration=gsrs.local",
    "start:gsrs:pre-prod": "npm run build-libraries && ng serve --aot=true --configuration=gsrs.pre-prod",
    "start:gsrs:prod": "npm run build-libraries && ng serve --aot=true --configuration=gsrs.prod",
    "start:inxight:dev": "npm run build-libraries && ng serve --aot=true --configuration=inxight.dev",
    "start:inxight:local": "npm run build-libraries && ng serve --aot=true --configuration=inxight.local",
    "start:inxight:pre-prod": "npm run build-libraries && ng serve --aot=true --configuration=inxight.pre-prod",
    "start:inxight:prod": "npm run build-libraries && ng serve --aot=true --configuration=inxight.prod",
    "build:abase:pre-prod": "npm run build-libraries && ng build --aot=true --configuration=abase.pre-prod && ng run gsrs-client:server && npm run webpack:server",
    "build:abase:prod": "npm run build-libraries && ng build --aot=true --configuration=abase.prod && ng run gsrs-client:server && npm run webpack:server",
    "build:fda:pre-prod": "npm run build-libraries && ng build --aot=true --configuration=fda.pre-prod && ng run gsrs-client:server && npm run webpack:server",
    "build:fda:prod": "npm run build-libraries && ng build --aot=true --configuration=fda.prod && ng run gsrs-client:server && npm run webpack:server",
    "build:gsrs:pre-prod": "npm run build-libraries && ng build --aot=true --configuration=gsrs.pre-prod && ng run gsrs-client:server && npm run webpack:server",
    "build:gsrs:prod": "npm run build-libraries && ng build --aot=true --configuration=gsrs.prod && ng run gsrs-client:server && npm run webpack:server",
    "build:inxight:pre-prod": "npm run ng build --aot=true --configuration=inxight.pre-prod && ng run gsrs-client:server && npm run webpack:server",
    "build:inxight:prod": "npm run build-libraries && ng build --aot=true --configuration=inxight.prod && ng run gsrs-client:server && npm run webpack:server",
    "build": "ng build --aot=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve:ssr": "node dist/server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/cdk": "6.4.6",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/material": "6.4.6",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/platform-server": "6.1.3",
    "@angular/router": "6.1.10",
    "@nguniversal/express-engine": "6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "6.0.0",
    "@types/hammerjs": "2.0.36",
    "classlist.js": "^1.1.20150312",
    "compression": "1.7.3",
    "core-js": "2.5.4",
    "dojo": "1.14.2",
    "dojox": "1.14.2",
    "file-select": "file:dist/file-select/file-select-0.0.1.tgz",
    "hammerjs": "2.0.8",
    "helmet": "3.13.0",
    "jsdraw-wrapper": "file:dist/jsdraw-wrapper/jsdraw-wrapper-0.0.1.tgz",
    "ketcher-wrapper": "file:dist/ketcher-wrapper/ketcher-wrapper-0.0.1.tgz",
    "lodash": "4.17.10",
    "rxjs": "6.0.0",
    "ts-loader": "4.5.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.5",
    "@angular-devkit/build-ng-packagr": "0.7.5",
    "@angular/cli": "6.1.5",
    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash": "4.14.116",
    "@types/node": "8.9.4",
    "codelyzer": "4.2.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.0",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "ng-packagr": "^3.0.0",
    "protractor": "5.4.0",
    "ts-node": "5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "webpack-cli": "3.2.1"
  }
}
