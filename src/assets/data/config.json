{
    "apiBaseUrl": "https://ginas.ncats.nih.gov/ginas/app/",
    "substanceDetailsCards": [
        {
            "card":"overview",
            "filters":[
                {
                    "filterName":"substanceClassFilter",
                    "propertyToCheck": "substanceClass",
                    "filterFunction": "equals",
                    "value": "chemical"
                }   
            ]            
        },
        {
            "card":"structure",
            "filters":[
                {
                    "filterName":"substanceClassFilter",
                    "propertyToCheck": "substanceClass",
                    "filterFunction": "equals",
                    "value": "chemical || polymer"
                },
                {
                    "filterName":"structurePropertyFilter",
                    "propertyToCheck": "structure",
                    "filterFunction": "exists"
                }
            ]            
        }
    ]
}
