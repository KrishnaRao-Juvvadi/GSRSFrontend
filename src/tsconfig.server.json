{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "outDir": "../out-tsc/app",
      "baseUrl": "./",
      "module": "commonjs",
      "types": [],
      "paths": {
        "@gsrs-core/*": [
          "app/core/*"
        ],
        "@environment": [
          "environments/environment.model.ts"
        ]
      }
    },
    "exclude": [
      "testing",
      "test.ts",
      "**/*.spec.ts",
      "testing/**/*"
    ],
    "angularCompilerOptions": {
      "entryModule": "app/core/app.server.module#AppServerModule"
    }
  }