{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "jsdraw-wrapper": [
        "dist/jsdraw-wrapper"
      ],
      "jsdraw-wrapper/*": [
        "dist/jsdraw-wrapper/*"
      ],
      "ketcher-wrapper": [
        "dist/ketcher-wrapper"
      ],
      "ketcher-wrapper/*": [
        "dist/ketcher-wrapper/*"
      ],
      "file-select": [
        "dist/file-select"
      ],
      "file-select/*": [
        "dist/file-select/*"
      ],
      "gsrs-core/dynamic-component-loader": [
        "src/app/core/dynamic-component-loader/dynamic-component-loader.module",
        "src/app/core/dynamic-component-loader/dynamic-component-loader.service",
        "src/app/core/dynamic-component-loader/dynamic-component-manifest",
        "src/app/core/dynamic-component-loader/*"
      ],
      "gsrs-core/base": [
        "src/app/core/base/base-http.service",
        "src/app/core/base/*"
      ],
      "gsrs-core/config": [
        "src/app/core/config/config.service",
        "src/app/core/config/*"
      ],
      "gsrs-core/google-analytics": [
        "src/app/core/google-analytics/track-link-event/track-link-event.directive",
        "src/app/core/google-analytics/google-analytics.service",
        "src/app/core/google-analytics/*"
      ],
      "gsrs-core/loading": [
        "src/app/core/loading/loading.module",
        "src/app/core/loading/loading.service",
        "src/app/core/loading/*"
      ],
      "gsrs-core/main-notification": [
        "src/app/core/main-notification/main-notification.module",
        "src/app/core/main-notification/main-notification.service",
        "src/app/core/main-notification/*"
      ],
      "gsrs-core/references-manager": [
        "src/app/core/references-manager/references-manager.module",
        "src/app/core/references-manager/references-manager.component",
        "src/app/core/references-manager/*"
      ],
      "gsrs-core/scroll-nav": [
        "src/app/core/scroll-nav/scroll-nav.module",
        "src/app/core/scroll-nav/*"
      ],
      "gsrs-core/structure": [
        "src/app/core/structure/structure-image-modal/structure-image-modal.component",
        "src/app/core/structure/structure-import/structure-import.component",
        "src/app/core/structure/structure.service",
        "src/app/core/structure/*"
      ],
      "gsrs-core/structure-editor": [
        "src/app/core/structure-editor/structure-editor-implementation.model",
        "src/app/core/structure-editor/structure-editor.component",
        "src/app/core/structure-editor/structure-editor.module",
        "src/app/core/structure-editor/structure.editor.model",
        "src/app/core/structure-editor/*"
      ],
      "gsrs-core/substance": [
        "src/app/core/substance/structural-unit.model",
        "src/app/core/substance/structure-search-response.model",
        "src/app/core/substance/substance-facet-param.model",
        "src/app/core/substance/substance-http-params",
        "src/app/core/substance/substance-utilities.model",
        "src/app/core/substance/substance.model",
        "src/app/core/substance/substance.service",
        "src/app/core/substance/*"
      ],
      "gsrs-core/substance-details": [
        "src/app/core/substance-details/substance-card-base",
        "src/app/core/substance-details/substance-card-base-filtered-list",
        "src/app/core/substance-details/substance-cards.service",
        "src/app/core/substance-details/substance-http-params",
        "src/app/core/substance-details/substance-utilities.model",
        "src/app/core/substance-details/substance.model",
        "src/app/core/substance-details/substance.service",
        "src/app/core/substance-details/*"
      ],
      "gsrs-core/top-search": [
        "src/app/core/top-search/top-search.component",
        "src/app/core/top-search/top-search.module",
        "src/app/core/top-search/top-search.service",
        "src/app/core/top-search/*",
      ],
      "gsrs-core/utils": [
        "src/app/core/utils/decode-html",
        "src/app/core/utils/facet.model",
        "src/app/core/utils/paging-response.model",
        "src/app/core/utils/structure-post-response.model",
        "src/app/core/utils/substance-suggestion.model",
        "src/app/core/utils/substance-suggestions-group.model",
        "src/app/core/utils/take.pipe",
        "src/app/core/utils/utils.service",
        "src/app/core/utils/vocabulary.model",
        "src/app/core/utils/*"
      ],
      "gsrs-core": [
        "src/app/core"
      ],
      "gsrs-core/*": [
        "src/app/core/*"
      ],
      "environment": [
        "src/environments/environment"
      ]
    }
  },
  "exclude": [
    "testing",
    "test.ts",
    "**/*.spec.ts",
    "testing/**/*"
  ]
}